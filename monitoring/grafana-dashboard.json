{
  "dashboard": {
    "id": null,
    "title": "Photon Neuromorphics SDK Dashboard",
    "tags": ["photon-neuro"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "System Performance",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(photon_neuro_operations_total[5m])",
            "legendFormat": "Operations/sec"
          }
        ]
      },
      {
        "id": 2,
        "title": "Memory Usage",
        "type": "graph",
        "targets": [
          {
            "expr": "photon_neuro_memory_usage_bytes",
            "legendFormat": "Memory (bytes)"
          }
        ]
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "refresh": "30s"
  }
}